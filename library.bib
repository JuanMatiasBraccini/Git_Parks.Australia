issn = {10557903},
journal = {Molecular Phylogenetics and Evolution},
keywords = {Australasia,Indo-Pacific,MYA,Mitochondrial DNA,Mustelus,Shark,Speciation,YBP},
number = {3},
pages = {697--703},
pmid = {22659513},
publisher = {Elsevier Inc.},
title = {{Genetic divergence, speciation and biogeography of Mustelus (sharks) in the central Indo-Pacific and Australasia}},
url = {http://dx.doi.org/10.1016/j.ympev.2012.05.024},
volume = {64},
year = {2012}
}
@article{Garla2006,
