journal = {Fisheries Oceanography},
keywords = {blue shark,hotspots,shortfin mako,spatio-temporal distribution,spatio-temporal model,template model builder},
number = {5},
pages = {569--582},
title = {{Predicting the spatio-temporal distributions of pelagic sharks in the western and central North Pacific}},
volume = {26},
