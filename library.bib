isbn = {1814232050950},
issn = {1814-232X},
journal = {African Journal of Marine Science},
keywords = {CPUE,DISTRIBUTION,EMBRYOS,GILLNETS,LENGTH FREQUENCY,LENGTH$\backslash$-WEIGHT RELATIONSHIPS,MATURITY,NURSERY GROUNDS,REPRODUCTION,SEASONALITY,STOMACH CONTENTS,TAGGING},
