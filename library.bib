journal = {PloS one},
keywords = {Algorithms,Animals,Australia,Chimera,Chimera: physiology,Conservation of Natural Resources,Ecosystem,Fisheries,Models,Oceans and Seas,Risk,Risk Assessment,Seafood,Sharks,Sharks: physiology,Skates (Fish),Skates (Fish): physiology,Species Specificity,Statistical},
month = {jan},
number = {2},
pages = {e32547},
pmid = {22384270},
title = {{High post-capture survival for sharks, rays and chimaeras discarded in the main shark fishery of Australia?}},
