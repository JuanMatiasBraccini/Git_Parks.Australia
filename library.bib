title = {{Coastal Habitat Use and Residency of Juvenile Atlantic Sharpnose Sharks ( {\textless}i{\textgreater}Rhizoprionodon terraenovae{\textless}/i{\textgreater} )}},
url = {http://dx.doi.org/10.1007/s12237-008-9075-2},
volume = {31},
