@article{Gedamke2007,
abstract = {Leslie matrices and life tables are demographic models commonly used to evaluate the ability of specific elasmobranch life history strategies to sustain given levels and patterns of fishing pressure. These models are generally density independent and provide an instantaneous rate of population growth for a specified set of life history traits that correspond to a specific population size. Many investigators are using these models to compute rates of population growth that they claim are estimates of the maximum population growth rate ({\textless}i{\textgreater}r{\textless}/i{\textgreater}{\textless}sub{\textgreater}intrinsic{\textless}/sub{\textgreater}); they then use these estimates to compute purported estimates of maximum sustainable fishing mortality. However, neither a Leslie matrix nor a life table can be used to estimate {\textless}i{\textgreater}r{\textless}/i{\textgreater}{\textless}sub{\textgreater}intrinsic{\textless}/sub{\textgreater} without additional information, except in the special case where a severely depleted population is modeled. Only in a severely depleted population will competition for resources be at a minimum and both density-dependent compensation and the rate of population growth be at a maximum (i.e., at {\textless}i{\textgreater}r{\textless}/i{\textgreater}{\textless}sub{\textgreater}intrinsic{\textless}/sub{\textgreater}). The fundamental problem is to determine the life history parameters that would occur if the population were extremely depleted because extensive observations on extremely depleted populations are rare. In the absence of such data, {\textless}i{\textgreater}r{\textless}/i{\textgreater}{\textless}sub{\textgreater}intrinsic{\textless}/sub{\textgreater} can only be estimated from these types of density-independent models by extrapolating observed population growth rates toward zero population size. We illustrate the problems in, and describe methods for, estimating {\textless}i{\textgreater}r{\textless}/i{\textgreater}{\textless}sub{\textgreater}intrinsic{\textless}/sub{\textgreater} and present information on two species of elasmobranch: barndoor skate {\textless}i{\textgreater}Dipturus laevis{\textless}/i{\textgreater} and lemon shark {\textless}i{\textgreater}Negaprion brevirostris{\textless}/i{\textgreater}},
author = {Gedamke, Todd and Hoenig, John M and Musick, John A and DuPaul, William D and Gruber, Samuel H},
issn = {0275-5947},
journal = {North American Journal of Fisheries Management},
keywords = {Elasmobranch,Fishing mortality,Growth,Mortality,Skate,competition,elasmobranchs,shark,size},
number = {2},
pages = {605--618},
publisher = {Taylor {\&} Francis},
title = {{Using Demographic Models to Determine Intrinsic Rate of Increase and Sustainable Fishing for Elasmobranchs: Pitfalls, Advances, and Applications}},
url = {http://www.informaworld.com/10.1577/M05-157.1},
volume = {27},
year = {2007}
}
