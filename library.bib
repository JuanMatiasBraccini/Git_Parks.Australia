@article{Biery2012,
abstract = {In this review, shark-fin-to-body-mass ratios, which have been legislated by several countries as a means of regulating and monitoring shark fisheries, have been compiled and reviewed. Observed and legislated wet-fin-mass-to-round-mass (M fw:M r) ratios have been collected for 50 species and eight countries. Wet to dry-fin mass conversion factors have also been reviewed. Existing shark fishery legislation was compiled by political entity and regional fishery management organizations (RFMO). The mean observed M fw:M r ratio for all species was 3{\textperiodcentered}0{\%}, but actual fin to body-mass ratios varied considerably by species and location. Species-specific mean ratios ranged from 1{\textperiodcentered}1 to 10{\textperiodcentered}9{\%}, and estimated mean ratios ranged from 1{\textperiodcentered}5 to 6{\textperiodcentered}1{\%} by country, depending on fin-cutting practices and the mix of exploited species. The mean conversion factor for wet to dry-fin mass was 0{\textperiodcentered}43. Shark-related legislation was found to exist in 37 countries and the 22 maritime members of the European Union, and shark-related regulations have been designated by nine RFMOs. Results suggest that currently regulated ratios may not be appropriate for all species and fin-cutting practices, and regulations based on generalized ratios for all sharks may be inadequate. Alternative policies may be necessary for the effective management of global shark fisheries. {\textcopyright} 2012 The Authors. Journal of Fish Biology {\textcopyright} 2012 The Fisheries Society of the British Isles.},
author = {Biery, L. and Pauly, D.},
doi = {10.1111/j.1095-8649.2011.03215.x},
