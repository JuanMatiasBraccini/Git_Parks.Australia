keywords = {Age and growth,Chondrichthyans,Elasmobranchs,von Bertalanffy},
number = {2-3},
pages = {230--235},
title = {{Use of stochastic models to estimate the growth of the Port Jackson shark, Heterodontus portusjacksoni, off eastern Victoria, Australia}},
volume = {95},
year = {2009}
