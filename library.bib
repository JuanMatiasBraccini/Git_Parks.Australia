month = {may},
number = {1},
pages = {1--11},
publisher = {Springer Netherlands},
title = {{The Seasonal Aggregation of Whale Sharks at Ningaloo Reef, Western Australia: Currents, Migrations and the El Ni+�o/ Southern Oscillation}},
