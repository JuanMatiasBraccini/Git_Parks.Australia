issn = {01657836},
journal = {Fisheries Research},
keywords = {Catch shares,Fisher behavior,Generalized additive model,Gulf of Mexico,IFQ,Management strategy evaluation},
number = {May},
pages = {85--94},
publisher = {Elsevier},
title = {{Vessel monitoring systems (VMS) reveal an increase in fishing efficiency following regulatory changes in a demersal longline fishery}},
