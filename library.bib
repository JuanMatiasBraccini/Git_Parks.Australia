number = {December},
pages = {105501},
publisher = {Elsevier Ltd},
title = {{Risk-based weight of evidence assessment of commercial sharks in western Australia}},
url = {https://doi.org/10.1016/j.ocecoaman.2020.105501},
year = {2020}
}
@article{Hill1997,
