file = {:C$\backslash$:/Matias/References/Freitas et al 2006.pdf:pdf},
issn = {00221112},
journal = {Journal of Fish Biology},
keywords = {Carcharhinidae,Chondrichthyes,Fabens method,Nursery,Tag-recapture},
number = {5},
pages = {1319--1332},
title = {{Early growth and juvenile population structure of lemon sharks Negaprion brevirostris in the Atol das Rocas Biological Reserve, off north-east Brazil}},
volume = {68},
year = {2006}
}
