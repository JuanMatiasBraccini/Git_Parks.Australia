journal = {Canadian Journal of Fisheries and Aquatic Sciences},
number = {9},
pages = {1554--1568},
title = {{A Bayesian mark–recapture model for multiple-recapture data in a catch-and-release fishery}},
volume = {66},
year = {2009}
}
