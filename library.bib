pages = {977--984},
publisher = {Springer Berlin / Heidelberg},
title = {{Annual re-sightings of photographically identified white sharks ({\textless}i{\textgreater}Carcharodon carcharias){\textless}/i{\textgreater} at an eastern Pacific aggregation site (Guadalupe Island, Mexico)}},
url = {http://dx.doi.org/10.1007/s00227-006-0380-7},
volume = {150},
