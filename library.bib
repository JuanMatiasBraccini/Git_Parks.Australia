pages = {1175--1184},
publisher = {Springer Berlin / Heidelberg},
title = {{Movements and environmental preferences of the shortfin mako, {\textless}i{\textgreater}Isurus oxyrinchus{\textless}/i{\textgreater}, in the southeastern Pacific Ocean}},
