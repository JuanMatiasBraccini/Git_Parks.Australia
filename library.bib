pages = {74--85},
title = {{The influence of sex and maturity on the diet, mouth morphology and dentition of the Port Jackson shark, Heterodontus portusjacksoni}},
url = {http://dx.doi.org/10.1071/MF09021},
volume = {61},
year = {2010}
}
