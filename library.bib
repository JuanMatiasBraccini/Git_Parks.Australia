journal = {Aquatic Living Resources},
keywords = {Atlantic Ocean,Bycatch,Discards,Purse seining,Tuna fisheries},
number = {4},
pages = {353--362},
title = {{Bycatch of the European purse seine tuna fishery in the Atlantic Ocean for the 2003-2007 period}},
volume = {23},
year = {2010}
