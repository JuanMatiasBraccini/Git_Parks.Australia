keywords = {Africa,Australia,Bycatch,Catch,Effort,Environment,Mortality,NETS,New South Wales,QUEENSLAND,Shark attacks,attack,attacks,by-catch,bycatch,drumlines,gill-nets,mortalities,.,fatal,fishing,large sharks,rays,shark,sharks,south africa},
month = {jun},
number = {6},
pages = {700--709},
title = {{Reducing the environmental impact of shark-control programs: a case study from KwaZulu-Natal, South Africa}},
url = {http://dx.doi.org/10.1071/MF10182},
volume = {62},
year = {2011}
}
