pages = {210--220},
title = {{Catch rate standardization for yellowfin tuna (Thunnus albacares) in Taiwan's distant-water longline fishery in the Western and Central Pacific Ocean, with consideration of target change}},
url = {http://www.sciencedirect.com/science/article/B6T6N-51GHWP2-6/2/107dd3100b1a78bc78a678f9923e8d83},
