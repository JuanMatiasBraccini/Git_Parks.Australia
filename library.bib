isbn = {978-0-387-78150-1},
issn = {00064971},
keywords = {integrated analysis,kalman filter,mark-recapture-recovery data,multistate models,phalacrocorax carbo sinensis,recruitment,state-space models},
pages = {917--946},
pmid = {17284531},
publisher = {Springer US},
title = {{Comparison of fixed effect, random effect, and hierarchical Bayes estimators for mark recapture data using AD Model Builder}},
