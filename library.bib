@article{Drew2015,
abstract = {Indonesia has the greatest reported chondrichthyan catches worldwide, with c.110,000t caught annually. The pelagic thresher (Alopias pelagicus) and scalloped hammerhead (Sphryna lewini) together comprise about 25{\%} of the total catches of sharks landed in Indonesia. Age and growth parameters were estimated for A. pelagicus and S. lewini from growth-band counts of thin-cut vertebral sections. Alopias pelagicus (n = 158) and S. lewini (n = 157) vertebrae were collected from three Indonesian fish markets over a 5 year period. A multi-model analysis was used to estimate growth parameters for both species. The models of best fit for males and females for A. pelagicus was the three-parameter logistic (L∞ = 3169mm LT, k = 0{\textperiodcentered}2) and the two-parameter von Bertalanffy models (L∞ = 3281mm LT, k = 0{\textperiodcentered}12). Age at maturity was calculated to be 10{\textperiodcentered}4 and 13{\textperiodcentered}2years for males and females, respectively, and these are the oldest estimated for this species. The samples of S. lewini were heavily biased towards females, and the model of best fit for males and females was the three-parameter Gompertz (L∞ = 2598mm LT, k = 0{\textperiodcentered}15) and the two-parameter Gompertz (L∞ = 2896mm LT, k= 0{\textperiodcentered}16). Age at maturity was calculated to be 8{\textperiodcentered}9 and 13{\textperiodcentered}2years for males and females, respectively. Although numerous age and growth studies have previously been undertaken on S. lewini, few studies have been able to obtain adequate samples from all components of the population because adult females, adult males and juveniles often reside in different areas. For the first time, sex bias in this study was towards sexually mature females, which are commonly lacking in previous biological studies on S. lewini. Additionally, some of the oldest aged specimens and highest age at maturity for both species were observed in this study. Both species exhibit slow rates of growth and late age at maturity, highlighting the need for a re-assessment of the relative resilience of these two globally threatened sharks at current high levels of fishing mortality throughout the eastern Indian Ocean.},
author = {Drew, M. and White, W. T. and Dharmadi and Harry, A. V. and Huveneers, C.},
doi = {10.1111/jfb.12586},
file = {:C$\backslash$:/Matias/References/Drew et al 2015.pdf:pdf},
issn = {10958649},
journal = {Journal of Fish Biology},
keywords = {Chondrichthyan,Fishing,Gompertz,Growth models,Indonesia,Von Bertalanffy},
number = {1},
pages = {333--354},
title = {{Age, growth and maturity of the pelagic thresher Alopias pelagicus and the scalloped hammerhead Sphyrna lewini}},
volume = {86},
year = {2015}
}
