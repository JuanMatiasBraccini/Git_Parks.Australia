journal = {Fisheries Research},
keywords = {AdaBoost,Bycatch,Catch,Catch rate,Delta model,Fish,Fishery,Generalized linear model,Tweedie distribution,Uncertainty,Zero catch,error,fisheries,generalized additive models,gillnet,linear model},
month = {jan},
number = {1-3},
pages = {261--271},
title = {{Decreasing uncertainty in catch rate analyses using Delta-AdaBoost: An alternative approach in catch and bycatch analyses with high percentage of zeros}},
url = {http://www.sciencedirect.com/science/article/B6T6N-51H1DCS-3/2/deb19043a602e4b9cc2658db2aef3186},
