isbn = {1215421109},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
number = {1},
pages = {178--183},
pmid = {27994155},
title = {{Fisheries management impacts on target species status}},
url = {http://www.pnas.org/lookup/doi/10.1073/pnas.1609915114},
volume = {114},
