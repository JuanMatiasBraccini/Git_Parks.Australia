publisher = {Springer Netherlands},
title = {{Investigations of {\textless}b{\textgreater}+�{\textless}/b{\textgreater}{\textless}sup{\textgreater}14{\textless}/sup{\textgreater}C, {\textless}b{\textgreater}+�{\textless}/b{\textgreater}{\textless}sup{\textgreater}13{\textless}/sup{\textgreater}C, and {\textless}b{\textgreater}+�{\textless}/b{\textgreater}{\textless}sup{\textgreater}15{\textless}/sup{\textgreater}N in vertebrae of white shark ({\textless}i{\textgreater}Carcharodon carcharias{\textless}/i{\textgreater}) from the easter}},
