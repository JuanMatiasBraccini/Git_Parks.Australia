file = {:C$\backslash$:/Users/myb/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kohler, Turner - 2001 - Shark tagging A review of conventional methods and studies.pdf:pdf},
issn = {0378-1909},
journal = {Environmental Biology of Fishes},
keywords = {Biomedical and Life Sciences,Fishery,Fishery management,Migrations,Mortality,Population dynamics,Tagging,detection,management,methods,migration,movement,shark},
month = {feb},
number = {1},
pages = {191--224},
publisher = {Springer Netherlands},
title = {{Shark tagging: A review of conventional methods and studies}},
url = {http://dx.doi.org/10.1023/A:1007679303082},
volume = {60},
year = {2001}
}
