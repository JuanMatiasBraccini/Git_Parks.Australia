journal = {Fisheries Research},
keywords = {Age,Age and growth,Bycatch,Exploitation,Growth,Growth parameter,Length-frequency analysis,Mortality,Natural mortality,Northeastern Taiwan,Okamejei acutispina,Robertson growth function,Skate,Stock assessment,length frequency,management,maturity},
month = {mar},
