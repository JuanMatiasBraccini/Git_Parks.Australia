number = {3},
pages = {325--337},
title = {{First Descriptions of the Behavior of Silky Sharks, Carcharhinus Falciformis, Around Drifting Fish Aggregating Devices in the Indian Ocean}},
url = {http://www.ingentaconnect.com/content/umrsmas/bullmar/2011/00000087/00000003/art00003 http://dx.doi.org/10.5343/bms.2010.1057},
