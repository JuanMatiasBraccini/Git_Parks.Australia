issn = {0378-1909},
journal = {Environmental Biology of Fishes},
keywords = {Biomedical and Life Sciences,depth,detection,migration,movement,residency,shark},
month = {jun},
number = {2},
pages = {165--176},
publisher = {Springer Netherlands},
title = {{Residency of the scalloped hammerhead shark ({\textless}i{\textgreater}Sphyrna lewini{\textless}/i{\textgreater}) at Malpelo Island and evidence of migration to other islands in the Eastern Tropical Pacific}},
