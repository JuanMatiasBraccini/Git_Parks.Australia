@article{Zeller2008,
abstract = {The bottom-fish stocks of the Main Hawaiian Islands (MHI) are intensively fished, both commercially and recreationally. Recent assessments of the Bottomfish Management Unit Species (BMUS) complex suggested overfishing, and expressed concerns about missing non-commercial data. We used reported commercial time-series data and estimation ratios to indirectly estimate non-commercial catches for non-pelagic species (i.e., excluding tuna and billfishes) for 1950-2005. Using adjustment ratios, we also accounted for commercial under-reporting, which suggested that total commercial non-pelagic catches were 28-128{\%} higher than reported commercial catches for any given year. Estimated non-commercial catches for 1950-2005 were 2.1 times higher than reported commercial catches. Reported catches underestimated likely total catches (reported and un-reported commercial plus non-commercial) of non-pelagic species and BMUS components for 1950-2005 by a factor of 3.9 and 2.9, respectively. We incorporated the reconstructed BMUS non-commercial catches into stock assessments of the officially reported commercial BMUS catches via a Schaefer production model. Total catch increased by 2.5-3.5 times with the addition of non-commercial BMUS catch estimates, which in turn increased model estimates of MSY and carrying capacity (k) by approximately four times compared to analyses with reported commercial data alone. As the CPUE data lacked information to resolve the confounding between large, unproductive and small, productive stocks, an informative prior was used for fishing mortality rate to attain MSY (FMSY). To address uncertainty in key management parameters, independent estimates of exploitation rate, or fisheries independent estimates of abundance, and informative trends in recreational effort or catches are required. {\textcopyright} 2007 Elsevier B.V. All rights reserved.},
author = {Zeller, D. and Darcy, M. and Booth, S. and Lowe, M. K. and Martell, S.},
doi = {10.1016/j.fishres.2007.11.010},
file = {:C$\backslash$:/Matias/References/Zeller et al 2008.pdf:pdf},
issn = {01657836},
journal = {Fisheries Research},
keywords = {Bayesian analysis,Bottom-fish,Catch reconstruction,Main Hawaiian Islands,Recreational fisheries,Stock assessment,Tropical fisheries},
number = {1},
pages = {88--97},
title = {{What about recreational catch?. Potential impact on stock assessment for Hawaii's bottomfish fisheries}},
volume = {91},
year = {2008}
}
