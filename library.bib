@article{Thorson2011,
abstract = {Catch-curve analysis is one of the simplest methods for stock assessment and is widely applied in data-poor fisheries. Conventional catch-curve methods rely on the strong assumptions of constant fishing and natural mortality rates above some fully selected age that is usually estimated by visually inspecting a plot of catch at age. Here, we evaluate the performance of three catch-curve methods that relax or modify these assumptions by (1) estimating logistic selectivity parameters, (2) assuming Lorenzen-form natural mortality (natural mortality that decreases with weight), and (3) using both methods simultaneously. We used simulation modeling and decision tables to compare estimates of fishing mortality from four catch-curve methods, including the conventional method, across a variety of observable and unobservable data characteristics. We then applied the methods to catch-at-age data for Atlantic menhaden {\textless}i{\textgreater}Brevoortia tyrannus{\textless}/i{\textgreater} from the U.S. South Atlantic fishery management region and compared the resulting estimates with published estimates of fishing mortality ({\textless}i{\textgreater}F{\textless}/i{\textgreater}). In our simulation modeling, catch curves that estimated logistic selectivity parameters performed better than those derived by the conventional method when logistic selectivity was present. There was generally little difference in performance between estimates assuming constant natural mortality and those assuming Lorenzen natural mortality. The improvements from estimating selectivity parameters were particularly pronounced when the sample sizes for catch-at-age data were large: in those instances, estimating selectivity improved the estimation accuracy for {\textless}i{\textgreater}F{\textless}/i{\textgreater} by nearly 20{\%}. In our example involving Atlantic menhaden, estimates of {\textless}i{\textgreater}F{\textless}/i{\textgreater} assuming logistic selectivity were most similar to those of published stock assessments, which had previously estimated logistic selectivity at age. We recommend our logistic-selectivity catch curve when selectivity is likely to be logistic because it improves accuracy at only a very small cost in terms of computational complexity},
author = {Thorson, James T and Prager, Michael H},
issn = {0002-8487},
journal = {Transactions of the American Fisheries Society},
keywords = {Age,Catch,Catch-at-age,Fishery,Fishery management,Fishing mortality,Mortality,Natural mortality,Selectivity,Stock assessment,accuracy,fisheries,management},
number = {2},
pages = {356--366},
publisher = {Taylor {\&} Francis},
title = {{Better Catch Curves: Incorporating Age-Specific Natural Mortality and Logistic Selectivity}},
url = {http://www.informaworld.com/10.1080/00028487.2011.557016},
volume = {140},
year = {2011}
}
