issn = {00221112},
journal = {Journal of Fish Biology},
keywords = {Elasmobranch,Finning,Laws,Policies and regulations,RFMO},
number = {5},
pages = {1643--1677},
pmid = {22497402},
title = {{A global review of species-specific shark-fin-to-body-mass ratios and relevant legislation}},
volume = {80},
year = {2012}
}
