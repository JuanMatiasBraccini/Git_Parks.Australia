@article{Tremblay2009,
abstract = {{\textless}p{\textgreater}Animal tracking is a growing field in ecology and previous work has shown that simple speed filtering of tracking data is not sufficient and that improvement of tracking location estimates are possible. To date, this has required methods that are complicated and often time-consuming (state-space models), resulting in limited application of this technique and the potential for analysis errors due to poor understanding of the fundamental framework behind the approach. We describe and test an alternative and intuitive approach consisting of bootstrapping random walks biased by forward particles. The model uses recorded data accuracy estimates, and can assimilate other sources of data such as sea-surface temperature, bathymetry and/or physical boundaries. We tested our model using ARGOS and geolocation tracks of elephant seals that also carried GPS tags in addition to PTTs, enabling true validation. Among pinnipeds, elephant seals are extreme divers that spend little time at the surface, which considerably impact the quality of both ARGOS and light-based geolocation tracks. Despite such low overall quality tracks, our model provided location estimates within 4.0, 5.5 and 12.0 km of true location 50{\%} of the time, and within 9, 10.5 and 20.0 km 90{\%} of the time, for above, equal or below average elephant seal ARGOS track qualities, respectively. With geolocation data, 50{\%} of errors were less than 104.8 km ({\textless}0.94A�), and 90{\%} were less than 199.8 km ({\textless}1.80A�). Larger errors were due to lack of sea-surface temperature gradients. In addition we show that our model is flexible enough to solve the obstacle avoidance problem by assimilating high resolution coastline data. This reduced the number of invalid on-land location by almost an order of magnitude. The method is intuitive, flexible and efficient, promising extensive utilization in future research.{\textless}/p{\textgreater}},
author = {Tremblay, Yann and Robinson, Patrick W and Costa, Daniel P},
file = {:C$\backslash$:/Users/myb/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Tremblay, Robinson, Costa - 2009 - A parsimonious approach to modeling animal movement data.pdf:pdf},
journal = {PLoS ONE},
keywords = {Animal movement,Tag,accuracy,ecology,error,geolocation,methods,model,models,movement,sea surface temperature,speed,temperature},
month = {mar},
number = {3},
pages = {e4711},
publisher = {Public Library of Science},
title = {{A parsimonious approach to modeling animal movement data}},
url = {http://dx.doi.org/10.1371/journal.pone.0004711},
volume = {4},
year = {2009}
}
