journal = {Fisheries Research},
keywords = {Blue shark,Pelagic sharks,Pre-recruit survival,Prionace glauca,Steepness,Stock-recruitment relationship},
number = {October},
title = {{Stock-recruitment relationships in elasmobranchs: Application to the North Pacific blue shark}},
year = {2018}
}
