journal = {Marine and Freshwater Research},
keywords = {Age,Age and growth,Australia,Catch,Growth,Growth parameter,Tagging,gillnet,length frequency,shark,sharks,tag recapture,tag-recapture,von Bertalanffy},
month = {jan},
number = {4},
pages = {417--433},
title = {{Age and growth of two commercially imported sharks (Carcharhinus tilstoni and C. sorrah) from Northern Australia}},
