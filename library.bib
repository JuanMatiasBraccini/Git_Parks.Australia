doi = {10.1093/icesjms/fsm179},
file = {:C$\backslash$:/Users/myb/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bishop et al. - 2008 - Standardizing catch rates Is logbook information by itself enough.pdf:pdf},
