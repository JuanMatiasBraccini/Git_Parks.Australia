issn = {1095-8649},
journal = {Journal of Fish Biology},
keywords = {Australia,Dasyatidae,Elasmobranch,Mortality,Port Jackson shark,conservation,elasmobranchs,management,maturity,oviposition,predation,predation rate,predator,shark},
