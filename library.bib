pages = {898--903},
title = {{Assessing growth band counts from vertebrae and dorsal-fin spines for ageing sharks: comparison of four methods applied to Heterodontus portusjacksoni}},
url = {http://dx.doi.org/10.1071/MF08206},
volume = {60},
year = {2009}
}
