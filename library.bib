journal = {Fisheries Research},
keywords = {Automatic longlines,Fisheries bycatch,Fishing mortality,Tag predation},
pages = {188--198},
publisher = {Elsevier B.V.},
title = {{Post-release survival, movement, and habitat use of school shark Galeorhinus galeus in the Great Australian Bight, southern Australia}},
url = {http://dx.doi.org/10.1016/j.fishres.2016.11.011},
volume = {187},
year = {2017}
}
