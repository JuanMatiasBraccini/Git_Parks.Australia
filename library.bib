issn = {10543139},
journal = {ICES Journal of Marine Science},
keywords = {Data storage tags,Mark-recapture experiments,North Sea,Plaice,Pleuronectes platessa},
number = {2},
pages = {236--246},
title = {{Do tagging experiments tell the truth? Using electronic tags to evaluate conventional tagging data}},
volume = {62},
year = {2005}
}
